# Token Service Configuration
# Copy this file to .env and fill in your actual values

# Firebase Cloud Functions endpoint for token validation
OURLIBRARY_TOKEN_ENDPOINT=https://us-central1-your-project.cloudfunctions.net/issueDownloadUrlHttp

# Alternative HTTP endpoint (if different from above)
OURLIBRARY_TOKEN_HTTP_ENDPOINT=https://us-central1-your-project.cloudfunctions.net/issueDownloadUrlHttp

# Firebase service account path (for admin operations)
FIREBASE_SERVICE_ACCOUNT_PATH=/path/to/your/serviceAccount.json

# Drive service configuration (usually handled by token service)
GOOGLE_DRIVE_FOLDER_ID=your-drive-folder-id
GOOGLE_SERVICE_ACCOUNT_EMAIL=your-service-account@your-project.iam.gserviceaccount.com